{
	"name": "@ctx-core/word-segmentation",
	"version": "6.0.0",
	"description": "Splits compound words into separate words using aspell",
	"keywords": [
		"ctx-core",
		"words",
		"aspell"
	],
	"homepage": "https://github.com/ctx-core/word-segmentation#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/word-segmentation/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/word-segmentation/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^22.0.0",
		"@ctx-core/function": "workspace:^19.0.0",
		"@ctx-core/queue": "workspace:^5.0.0",
		"@ctx-core/string": "workspace:^8.0.0",
		"esm": "^3.2.25",
		"memory-streams": "^0.1.3"
	},
	"devDependencies": {
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
