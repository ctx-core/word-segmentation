{
	"name": "@ctx-core/word-segmentation",
	"version": "7.0.20",
	"description": "Splits compound words into separate words using aspell",
	"keywords": [
		"ctx-core",
		"words",
		"aspell"
	],
	"homepage": "https://github.com/ctx-core/word-segmentation#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/word-segmentation/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/word-segmentation/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"word-segmentation": "./bin/word-segmentation.mjs"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.21",
		"@ctx-core/function": "workspace:^20.2.1",
		"@ctx-core/queue": "workspace:^6.0.13",
		"@ctx-core/string": "workspace:^9.0.19",
		"esm": "^3.2.25",
		"memory-streams": "^0.1.3"
	},
	"devDependencies": {
		"typescript": "^4.4.3"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}