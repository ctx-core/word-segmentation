{
	"name": "@ctx-core/word-segmentation",
	"version": "7.2.32",
	"description": "Splits compound words into separate words using aspell",
	"keywords": [
		"ctx-core",
		"words",
		"aspell"
	],
	"homepage": "https://github.com/ctx-core/word-segmentation#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/word-segmentation/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/word-segmentation.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin",
		"./_types",
		"./Aspell",
		"./aspell_",
		"./aspell_line_compound_title_case_",
		"./backward_word_reduction_compound_word_aa_",
		"./compound_line",
		"./compound_word_a_",
		"./compound_words_stream_a_",
		"./CompoundAspell",
		"./segment_words",
		"./token_a_",
		"./top_Aspell",
		"./top_line",
		"./word_reduction_compound_word_a_",
		"./word_segmentation_streams_",
		"./word_segmentation_streams_close"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"word-segmentation": "./bin/word-segmentation.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^27.0.32",
		"@ctx-core/function": "^21.22.27",
		"@ctx-core/queue": "^7.10.32",
		"@ctx-core/string": "^12.4.32",
		"memory-streams": "^0.1.3"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.6.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}