{
	"name": "@ctx-core/word-segmentation",
	"version": "7.0.47",
	"description": "Splits compound words into separate words using aspell",
	"keywords": [
		"ctx-core",
		"words",
		"aspell"
	],
	"homepage": "https://github.com/ctx-core/word-segmentation#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/word-segmentation/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/word-segmentation.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"word-segmentation": "./bin/word-segmentation.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^24.0.0",
		"@ctx-core/function": "workspace:^20.3.0",
		"@ctx-core/queue": "workspace:^6.1.20",
		"@ctx-core/string": "workspace:^10.0.0",
		"memory-streams": "^0.1.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.133",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js",
	"sideEffects": false
}