{
	"name": "@ctx-core/word-segmentation",
	"version": "7.0.33",
	"description": "Splits compound words into separate words using aspell",
	"keywords": [
		"ctx-core",
		"words",
		"aspell"
	],
	"homepage": "https://github.com/ctx-core/word-segmentation#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/word-segmentation/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/word-segmentation/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"word-segmentation": "./bin/word-segmentation.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.34",
		"@ctx-core/function": "workspace:^20.2.13",
		"@ctx-core/queue": "workspace:^6.1.9",
		"@ctx-core/string": "workspace:^9.0.32",
		"memory-streams": "^0.1.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
